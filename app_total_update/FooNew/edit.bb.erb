<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @foonew.object %>
    <h1>Edit <%= @foonew.barnew1%></h1>
    <%= link_to "Delete", :action => :delete, :id => @foonew.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @foonew.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Barnew1: </td>
              <td class="itemValue"><input type="text" name="foonew[barnew1]" value="<%= @foonew.barnew1 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Barnew2: </td>
              <td class="itemValue"><input type="text" name="foonew[barnew2]" value="<%= @foonew.barnew2 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Barnew3: </td>
              <td class="itemValue"><input type="text" name="foonew[barnew3]" value="<%= @foonew.barnew3 %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
